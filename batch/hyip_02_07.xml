<?xml version="1.0"?>
<sweep runs="3">
<parameter name="generation_number" type="constant" constant_type="number" value="500"/>
<parameter name="iteration_number" type="constant" constant_type="number" value="200"/>
<parameter name="inv_0" type="constant" constant_type="number" value="500"/> 
<parameter name="inv_1" type="constant" constant_type="number" value="500"/>
<parameter name="inv_2" type="constant" constant_type="number" value="500"/>
<parameter name="consumer_max_threshold_mutation" type="constant" constant_type="number" value="0.05"/>
<parameter name="income_eval_frompeek" type="constant" constant_type="java.lang.Boolean" value="false"/>

<parameter name="hyip_population_size" type="constant" constant_type="number" value="160"/>
<parameter name="hyip_perc" type="constant" constant_type="number" value="0.02"/> 
<parameter name="hyip_marketing" type="constant" constant_type="number" value="0"/>
<parameter name="hyip_look" type="constant" constant_type="number" value="1"/>

<parameter name="e_cost" type="constant" constant_type="number" value="50"/>
<parameter name="p_cost" type="constant" constant_type="number" value="150"/>

<parameter name="first_strategy_bias" type="constant" constant_type="number" value="20"/>
<parameter name="mutate_chance" type="constant" constant_type="number" value="1"/>
<parameter name="mutate_factor" type="constant" constant_type="number" value="0.05"/>

<parameter name="e_eff" type="constant" constant_type="number" value="0.01"/>
<parameter name="p_eff" type="constant" constant_type="number" value="0.02"/>
<parameter name="l_eff" type="constant" constant_type="number" value="0.05"/>
<parameter name="verbose" type="constant" constant_type="java.lang.Boolean" value="false"/>

<parameter name="l_cost" type="constant" constant_type="number" value="2500"/>
<parameter name="l_cost_rand" type="constant" constant_type="java.lang.Boolean" value="true"/>

<parameter name="inv_invest" type="constant" constant_type="number" value="0.1"/>
<parameter name="inv_rec" type="constant" constant_type="number" value="0.1"/>
<parameter name="unlimited_i_funds" type="constant" constant_type="java.lang.Boolean" value="true"/>
</sweep>